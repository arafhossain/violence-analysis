{
    "extends": "airbnb",
    "parserOptions":{
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "new-cap": 0,
      "react/react-in-jsx-scope": 0,
      "no-unused-vars": ["error", { "varsIgnorePattern": "html" }]
    }
}
